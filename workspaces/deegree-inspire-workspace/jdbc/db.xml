<?xml version='1.0' encoding='UTF-8'?>
<JDBCConnection xmlns="http://www.deegree.org/jdbc" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" configVersion="3.4.0" xsi:schemaLocation="http://www.deegree.org/jdbc ../appschemas/schemas.deegree.org/jdbc/3.4.0/jdbc.xsd">

  <!-- [1] JDBC URL (without username / password) -->
  <Url>jdbc:postgresql://myHost:5432/db</Url>

  <!-- [1] DB username -->
  <User>DB_USER</User>

  <!-- [1] DB password -->
  <Password>DB_PASSWORD</Password>

</JDBCConnection>